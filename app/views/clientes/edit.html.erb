<hr>
<%= form_for @clientes do |f| %>
<div class="container">
    <table class="table table-dark">
        <tr>
            <td>
                <%= f.label :rfc %>
            </td>
        </tr>
        <tr>
            <td>
                <%= f.text_field :rfc %>
            </td>
        </tr>
        <tr>
            <td>
                <%= f.label :nombre_cliente %>
            </td>
        </tr>
        <tr>
            <td>
                <%= f.text_field :nombre_cliente %>
            </td>
        </tr>
        <tr>
            <td>
                <%= f.submit %>
            </td>
        </tr>
    </table>
</div>

<%end %>